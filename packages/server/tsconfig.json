{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext"],
    "allowJs": false,
    "checkJs": false,
    "outDir": "./dist"
  },
  "include": ["./src/**/*"]
}
